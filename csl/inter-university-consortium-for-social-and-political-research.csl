  <info>
    <title>Inter-university Consortium for Social and Political Research</title>
    <title-short>ICPSR</title-short>
    <id>http://www.zotero.org/styles/inter-university-consortium-for-social-and-political-research</id>
    <link href="http://www.zotero.org/styles/inter-university-consortium-for-social-and-political-research" rel="self"/>
    <author>
      <name>Inter-university Consortium for Social and Political Research</name>
      <uri>https://icpsr.umich.edu</uri>
      <email>icpsr-metadata@umich.edu</email>
