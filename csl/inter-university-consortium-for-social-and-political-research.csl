  <citation>
    <layout>
      <names variable="author" delimiter="," suffix=". ">
        <name and="text" name-as-sort-order="all"/>
      </names>
      <date form="numeric" variable="issued" suffix=". "/>
    </layout>
  </citation>
  <bibliography>
    <layout>
      <names variable="author" delimiter="," suffix=". ">
        <name and="text" name-as-sort-order="all"/>
      </names>
      <text variable="title" suffix=". "/>
      <group suffix=", ">
        <choose>
          <if variable="seconddistributor">
            <text variable="publisher" suffix=", "/>
            <text variable="seconddistributor" suffix=" "/>
            <text value="[distributors]"/>
          </if>
          <else>
            <text variable="publisher" suffix=" "/>
            <text value="[distributor]"/>
          </else>
        </choose>
      </group>
      <date form="numeric" variable="issued" suffix=". "/>
      <text variable="DOI"/>
    </layout>
  </bibliography>
